alias sail='[ -f sail ] && bash sail || bash vendor/bin/sail'
alias art='sail artisan'
alias sailinit='docker run --rm \
    -u "$(id -u):$(id -g)" \
    -v $(pwd):/var/www/html \
    -w /var/www/html \
    laravelsail/php80-composer:latest \
    '
alias dcomposer='docker run --rm \
    -u "$(id -u):$(id -g)" \
    -v $(pwd):/var/www/html \
    -w /var/www/html \
    composer:latest \
    composer '
alias cd..='cd ..'
